{
    "name": "monkeyphp/mphp-flickr-photo-get-sizes",
    "description": "Package for accessing the Flickr flickr.photos.getSizes api",
    "type": "library",
    "homepage": "https://github.com/monkeyphp/mphp-flickr-photo-get-sizes",
    "license": "MIT",
    "keywords": [
        "Flickr",
        "photos"
    ],
    "authors": [
        {
            "name": "David White",
            "email": "git@monkeyphp.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "git@monkeyphp.com"
    },
     "repositories": [
        {
            "type": "package",
            "package": {
                "name":"monkeyphp/mphp-flickr-base",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/monkeyphp/mphp-flickr-base",
                    "type": "git",
                    "reference": "master"
                },
                "autoload" : {
                    "psr-0": {"MphpFlickrBase": "src/"}
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "monkeyphp/mphp-flickr-base": "1.0.0",
        "zendframework/zend-http": "2.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.14"
    },
    "autoload": {
        "psr-0": {
            "MphpFlickrPhotoGetSizes": "src/"
        }
    }
}
